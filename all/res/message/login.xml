<?xml version="1.0" encoding="UTF-8"?>
<messages package="login" id="100">
	<bean name="Test" explain="道具信息">
		<field class="int" name="itemId" explain="道具model"></field>
		<field class="int" name="itemCount" explain="道具数量"></field>
	</bean>
	
	<!-- 服务器接收消息 -->
	<message id="101" type="CS" name="ReqLogin" explain="登录">
		<field class="String" name="accountName" explain="帐号名称"></field>
		<field class="int" name="server" explain="服务器id"></field>
		<field class="String" name="check" explain="验证key/密码"></field>
	</message>
	
	<!-- 服务器发送消息 -->
	<message id="201" type="SC" name="ResLoginFailed" explain="登录失败">
		<field class="byte" name="err" explain="错误代码"></field>
	</message>
</messages>